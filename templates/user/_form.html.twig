{{ form_start(form) }}
<section class="section">
    <div class="row">
        <div class="card">
            <div class="card-body">
                <br>
                {{ form_row(form.username) }}
                {{ form_row(form.password) }}
                {{ form_row(form.email) }}
                {{ form_row(form.picture) }}

                <button class="btn ">{{ button_label|default('Save') }}</button>
            </div>
        </div>
    </div>
</section>
{{ form_end(form) }}
